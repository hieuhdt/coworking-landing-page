{
  "bail": 1,
  "verbose": true,
  "testEnvironment": "jsdom",
  "moduleFileExtensions": ["js", "jsx"],
  "transform": {
    "^.+\\.(js|jsx)$": "babel-jest"
  },
  "moduleNameMapper": {
    "\\.(css|less|sass|scss)$": "identity-obj-proxy",
    "\\.(svg|png|jpg|jpeg|gif)$": "<rootDir>/_mocks_/fileMock.js"
  }
}
